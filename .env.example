NXCLAW_HOME=~/.nxclaw
NXCLAW_WORKSPACE=
NXCLAW_DEFAULT_PROVIDER=google-gemini-cli
NXCLAW_DEFAULT_MODEL=
NXCLAW_DASHBOARD_HOST=127.0.0.1
NXCLAW_DASHBOARD_PORT=3020
NXCLAW_CHANNELS=web,telegram,slack

NXCLAW_PROMPT_TIMEOUT_MS=300000
NXCLAW_MAX_PROMPT_RETRIES=2
NXCLAW_MAX_QUEUE_DEPTH=100
NXCLAW_MAX_CONCURRENT_TASKS=6
NXCLAW_TASK_RETRY_LIMIT=2
NXCLAW_TASK_RETRY_DELAY_MS=5000

NXCLAW_DIAGNOSTICS_ENABLED=true
NXCLAW_EVENT_BUFFER_SIZE=500

NXCLAW_AUTO_ENABLED=true
NXCLAW_AUTO_GOAL=Autonomously recover stalled tasks, clean up dead background jobs, compact memory, and continuously advance top-priority objectives with concrete progress updates.
NXCLAW_AUTO_INTERVAL_MS=90000
NXCLAW_AUTO_SKIP_QUEUE_ABOVE=2
NXCLAW_AUTO_MAX_FAILURES=5
NXCLAW_AUTO_STALE_PENDING_HOURS=336
NXCLAW_AUTO_STALE_IN_PROGRESS_IDLE_HOURS=72

NXCLAW_MEMORY_VECTOR_ENABLED=true
NXCLAW_MEMORY_VECTOR_CACHE_ENABLED=true
NXCLAW_MEMORY_VECTOR_PROVIDER=auto
NXCLAW_MEMORY_VECTOR_MODEL=text-embedding-3-small
NXCLAW_MEMORY_VECTOR_DIMS=256
NXCLAW_MEMORY_VECTOR_BATCH_SIZE=32
NXCLAW_MEMORY_VECTOR_WEIGHT=0.65
NXCLAW_MEMORY_TEXT_WEIGHT=0.35
NXCLAW_MEMORY_MIN_SCORE=0.12
NXCLAW_MEMORY_EXTRA_PATHS=
NXCLAW_MEMORY_SESSION_ENABLED=true

NXCLAW_SKILLS_ENABLED=true
NXCLAW_SKILLS_AUTO_ENABLE=true
NXCLAW_CODEX_SKILLS_DIR=~/.codex/skills
NXCLAW_SKILLS_MAX_PROMPT=6
NXCLAW_SKILLS_MAX_PROMPT_CHARS=8000

NXCLAW_SLACK_BOT_TOKEN=
NXCLAW_SLACK_APP_TOKEN=
NXCLAW_SLACK_SIGNING_SECRET=
NXCLAW_SLACK_CHANNELS=

NXCLAW_TELEGRAM_BOT_TOKEN=

ANTHROPIC_API_KEY=
OPENAI_API_KEY=
OPENAI_BASE_URL=
GEMINI_API_KEY=
GEMINI_BASE_URL=
NXCLAW_CHROME_MODE=launch
NXCLAW_CHROME_CDP_URL=http://127.0.0.1:9222
NXCLAW_CHROME_CDP_CONNECT_TIMEOUT_MS=15000
NXCLAW_CHROME_CDP_REUSE_EXISTING_PAGE=true
NXCLAW_CHROME_CDP_FALLBACK_TO_LAUNCH=true
NXCLAW_CHROME_HEADLESS=true
NXCLAW_CHROME_PATH=
NXCLAW_CHROME_MAX_SESSIONS=6
